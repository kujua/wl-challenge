{
  "default_stop_words": [
    "defmodule",
    "defrecord",
    "defimpl",
    "defexception",
    "defprotocol",
    "defstruct",
    "def.+(.+\\\\.+).+do",
    "^\\s+use\\s+"
  ],
  "skip_files": [
    "test",
    "views",
    "application.ex",
    "mock_miner.ex",
    "challenge.web.ex",
    "endpoint.ex",
    "gettext.ex",
    "router.ex",
    "telemetry.ex"
  ],
  "custom_stop_words": [],
  "coverage_options": {
    "treat_no_relevant_lines_as_covered": true,
    "output_dir": "cover/",
    "minimum_coverage": 85
  },
  "terminal_options": {
    "file_column_width": 40
  }
}